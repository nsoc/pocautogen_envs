ORACLE_DB_BASE=$INST_HOME/oracle
ORACLE_DB_ORACLE_HOME=$ORACLE_DB_BASE/product/11.2.0/dbhome_1
@P > ORACLE_SID=orclpoc
ORACLE_HOSTNAME=$ADMIN_NW_IPADDR
ORACLE_DB_INVENTORY_LOC=$INST_HOME/oraInventory_db1120
GLOBAL_DB_NAME=${ORACLE_SID}.oracle.co.jp
ORACLE_DB_DATA_LOC=$ORACLE_DB_BASE/oradata_${ORACLE_SID}
ORACLE_DB_MEMORY_MB=1024
@P > ORACLE_DB_LISTEN_PORT=1521
@P > ORACLE_JDBC_URL=jdbc:oracle:thin:@${ORACLE_DB_LISTEN_ADDR}:${ORACLE_DB_LISTEN_PORT}:${ORACLE_SID}
@P > ORACLE_USER=scott
@P > ORACLE_PASSWORD=tiger
@P > ORACLE_EZCONNECT=$ORACLE_HOSTNAME:$ORACLE_DB_LISTEN_PORT/$GLOBAL_DB_NAME
